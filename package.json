    "type": "git",
    "url": "https://github.com/geek-fun/dockit.git"
  },
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "cross-env APP_ENV=dev DEBUG=dockit ELECTRON_DISABLE_SECURITY_WARNINGS=true electron-forge start",
    "package": "cross-env APP_ENV=prod electron-forge package",
    "make": "cross-env APP_ENV=prod electron-forge make",
    "publish": "electron-forge publish",
    "lint:fix": "eslint --fix ./",
    "lint:check": "eslint ./",
    "test": "jest --runInBand --coverage --coverageReporters json-summary text html lcov",
    "test:ci": "jest --runInBand --ci --coverage --coverageReporters json-summary text html lcov"
  },
