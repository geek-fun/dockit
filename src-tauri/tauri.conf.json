{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "DocKit",
  "mainBinaryName": "DocKit",
  "version": "../package.json",
  "identifier": "club.geekfun.dockit",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "webviewInstallMode": {
        "type": "skip"
      }
    },
    "category": "DeveloperTool",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {},
  "app": {
    "macOSPrivateApi": false,
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "DocKit",
        "width": 1600,
        "height": 1000,
        "useHttpsScheme": true
      }
    ]
  }
}
